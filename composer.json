{
    "name": "beelab/mailchimp-bundle",
    "type": "symfony-bundle",
    "description": "Provide access to Mailchimp API v3",
    "keywords": ["Symfony", "Mailchimp"],
    "homepage": "https://bee-lab.net",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Mauro D'Alatri",
            "email": "mauro.dalatri@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=5.3.8",
        "symfony/symfony": "~2.3",
        "beelab/v3-mailchimp-api-php": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Beelab\\MailchimpBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
